@model List<CategoryDto>

@if (Model.Count>0) {
<div class="megamenu">
    <div class="row" >
        @foreach (var item in Model)
        {
            <div class="col-md-3">
                <h4 class="menu-title">
                    <a asp-action="GetCategoryId" asp-controller="Home" asp-route-category="@Url.FriendlyUrl(item.Name)">
                        @item.Name
                       
                    </a>
                </h4>

            </div>
        }


    </div>

</div>
}


